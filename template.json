{
  "log": {
    "loglevel": "warning"
  },
  "dns": {
    "queryStrategy": "UseIP",
    "servers": [
      "https://1.1.1.1/dns-query",
      "https://8.8.8.8/dns-query",
      "https://9.9.9.9/dns-query"
    ],
    "tag": "dns_out"
  },
  "fakedns": [
    {
      "ipPool": "198.18.0.0/15",
      "poolSize": 65535
    }
  ],
  "routing": {
    "balancers": [
      {
        "tag": "balancer",
        "selector": ["proxy"],
        "strategy": {
          "type": "leastPing"
        },
        "fallbackTag": "proxy1"
      }
    ],
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["socks"],
        "network": "udp",
        "port": 53,
        "outboundTag": "dns-out"
      },
      {
        "type": "field",
        "balancerTag": "balancer",
        "network": "tcp,udp"
      }
    ]
  },
  "policy": {
    "system": {
      "statsOutboundDownlink": true,
      "statsOutboundUplink": true
    }
  },
  "inbounds": [
    {
      "port": 10808,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true,
        "userLevel": 8
      },
      "sniffing": {
        "destOverride": ["http", "tls", "fakedns"],
        "enabled": false
      },
      "tag": "socks"
    },
    {
      "port": 10809,
      "protocol": "http",
      "settings": {
        "userLevel": 8
      },
      "tag": "http"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {
        "fragment": {
          "interval" : "0-1",
          "length" : "4-8",
          "packets" : "1-1"
        }
      },
      "tag": "dialer"
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIP"
      }
    },
    {
      "tag": "block",
      "protocol": "blackhole"
    },
    {
      "tag": "dns-out",
      "protocol": "dns"
    }
  ],
  "observatory": {
    "subjectSelector": ["proxy"],
    "probeURL": "http://www.google.com/gen_204",
    "probeInterval": "1m",
    "enableConcurrency": true
  },
  "stats": {},
  "remarks": "üåê Global (Optimized)"
}
