{
    "log": {
        "loglevel": "warning"
    },
    "dns": {
        "queryStrategy": "UseIP",
        "servers": [
            "1.1.1.1",
            "8.8.8.8",
            "1.0.0.1"
        ],
        "tag": "dns_out"
    },
    "fakedns": [
        {
            "ipPool": "198.18.0.0/15",
            "poolSize": 65535
        }
    ],
    "routing": {
        "balancers": [
            {
                "tag": "balancer",
                "selector": [
                    "proxy"
                ],
                "strategy": {
                    "type": "leastLoad"
                },
                "fallbackTag": "proxy1"
            }
        ],
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "outboundTag": "block",
                "protocol": [
                    "bittorrent"
                ],
                "type": "field"
            },
            {
                "type": "field",
                "inboundTag": [
                    "socks"
                ],
                "network": "udp",
                "port": 53,
                "outboundTag": "dns-out"
            },
            {
                "type": "field",
                "balancerTag": "balancer",
                "network": "tcp,udp"
            }
        ]
    },
    "policy": {
        "system": {
            "statsOutboundDownlink": true,
            "statsOutboundUplink": true
        }
    },
    "inbounds": [
        {
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": true,
                "userLevel": 8
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls",
                    "fakedns"
                ],
                "enabled": false
            },
            "tag": "socks"
        },
        {
            "port": 10809,
            "protocol": "http",
            "settings": {
                "userLevel": 8
            },
            "tag": "http"
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "fragment": {
                    "interval": "1-4",
                    "length": "10-100",
                    "packets": "2-4"
                },
                "noises": [
                    {
                        "delay": "5-10",
                        "packet": "5-15",
                        "type": "rand"
                    }
                ]
            },
            "tag": "dialer"
        },
        {
            "tag": "direct",
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIP"
            }
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        },
        {
            "tag": "dns-out",
            "protocol": "dns"
        }
    ],
    "observatory": {
        "subjectSelector": [
            "proxy"
        ],
        "probeURL": "https://cp.cloudflare.com/generate_204",
        "probeInterval": "5m",
        "enableConcurrency": true
    },
    "stats": {},
    "remarks": "üåê Global (Optimized)"
}